APP_NAME=FastAPI RBAC
APP_ENV=development
DATABASE_URL=mysql+pymysql://root:root@localhost:3306/fastapi_rbac
JWT_SECRET_KEY=change-this-secret-in-production
JWT_ALGORITHM=HS256
JWT_ACCESS_TOKEN_EXPIRE_MINUTES=60
CORS_ALLOWED_ORIGINS=*
CORS_ALLOW_CREDENTIALS=true
CORS_ALLOWED_METHODS=*
CORS_ALLOWED_HEADERS=*
UPLOAD_ROOT_DIR=storage/uploads
MAX_FILE_SIZE_BYTES=5242880
ATTENDANCE_MAX_DISTANCE_METERS=300
ATTENDANCE_FACE_PROVIDER=aws_rekognition
ATTENDANCE_FACE_MATCH_THRESHOLD=0.80
AWS_REGION=ap-south-1
AWS_ACCESS_KEY_ID=
AWS_SECRET_ACCESS_KEY=
